{
  "name": "middle-domain",
  "version": "1.2.4",
  "description": "",
  "private": true,
  "main": "dist/lib/middle-domain.js",
  "scripts": {
    "dev": "rollup -c --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn update:version && rollup -c rollup.config.prod.js && yarn update:sourcemap",
    "update:sourcemap": "node ./scripts/replace-source-map-url.js",
    "update:version": "node ./scripts/build-version-update.js",
    "format": "prettier --write './src/**/*.(vue|js|css|less|jsx)'",
    "serve": "http-server -p 10001 ./dist",
    "preserve": "rollup -c --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.1.0",
    "ali-oss-deploy": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "chalk": "4.0.0",
    "co": "^4.6.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "inquirer": "^8.2.2",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-uglify": "^6.0.4",
    "semver": "^7.3.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "node_modules/.bin/prettier --write",
      "git add ."
    ]
  },
  "dependencies": {
    "axios": "^0.25.0",
    "merge": "^2.1.1"
  }
}